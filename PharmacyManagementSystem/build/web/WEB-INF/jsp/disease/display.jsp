    </head>
    <body>
        <div class="container">
            <a href="${pageContext.request.contextPath}/display" class="btn btn-success"><span class="glyphicon glyphicon-home"></span></a>
            <h1>Disease List:</h1>
            <ul>
                <c:forEach var="dis" items="${disease}">
